{
  "name": "got-quotes-generator",
  "version": "1.0.0",
  "description": "A Game of Thrones Quotes Generator",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest"
  },
  "author": "Junyuan Xue",
  "license": "ISC",
  "dependencies": {
    "css-loader": "^0.23.1",
    "jquery": "^3.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-icons": "^2.1.0",
    "react-router": "^2.4.1",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-jest": "^13.0.0",
    "babel-preset-react": "^6.5.0",
    "html-webpack-plugin": "^2.21.0",
    "jest-cli": "^13.0.0",
    "react-addons-test-utils": "^15.1.0",
    "react-stub-context": "^0.7.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "testDirectoryName": "__test__",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ],
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "^config$": "<rootDir>/configs/app-config.js",
      "^.+\\.(css|less)$": "<rootDir>/test/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/fileMock.js"
    }
  }
}
